<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <fragment
        android:id="@+id/resourcesMap1"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="0dp"
        android:layout_height="290dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:context="compass.compass.MapsActivity" />

    <android.support.v7.widget.CardView
        android:id="@+id/clPolice1"
        android:layout_width="344dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resourcesMap1"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="8dp">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/ivCallPolice1"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginBottom="8dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_phone_black_24dp"
                android:layout_marginEnd="16dp" />

            <TextView
                android:id="@+id/tvPolice1"
                android:textColor="@color/Black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:paddingRight="15dp"
                android:text="TextView"
                app:layout_constraintLeft_toRightOf="@+id/ivPolicePin1"
                app:layout_constraintTop_toTopOf="@+id/ivPolicePin1"
                android:layout_marginStart="16dp" />

            <TextView
                android:id="@+id/tvPoliceDistance1"
                android:layout_width="wrap_content"
                android:layout_height="16dp"
                android:paddingRight="15dp"
                android:text="TextView"
                app:layout_constraintLeft_toLeftOf="@+id/tvPolice1"
                app:layout_constraintTop_toBottomOf="@+id/tvPolice1" />

            <ImageView
                android:id="@+id/ivPolicePin1"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                app:srcCompat="@drawable/ic_location_pin_blue"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="16dp"
                app:layout_constraintLeft_toLeftOf="parent" />
        </android.support.constraint.ConstraintLayout>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/cvCounseling1"
        android:layout_width="344dp"
        android:layout_height="70dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/clPolice1"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tvCounselingDistance1"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:text="TextView"
                app:layout_constraintLeft_toLeftOf="@+id/tvCounseling1"
                app:layout_constraintTop_toBottomOf="@+id/tvCounseling1" />

            <TextView
                android:id="@+id/tvCounseling1"
                android:textColor="@color/Black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="15dp"
                android:text="TextView"
                app:layout_constraintTop_toTopOf="@+id/ivCounselingPin1"
                app:layout_constraintLeft_toRightOf="@+id/ivCounselingPin1"
                android:layout_marginLeft="16dp"
                android:layout_marginStart="16dp" />

            <ImageView
                android:id="@+id/ivCallCounseling1"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginBottom="8dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_phone_black_24dp"
                android:layout_marginEnd="16dp" />

            <ImageView
                android:id="@+id/ivCounselingPin1"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:layout_margin="15dp"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_location_pin_green"
                android:layout_marginStart="16dp" />
        </android.support.constraint.ConstraintLayout>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/cvHospital1"
        android:layout_width="344dp"
        android:layout_height="70dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/cvCounseling1"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tvHospitalDistance1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                app:layout_constraintTop_toBottomOf="@+id/tvHospital1"
                app:layout_constraintLeft_toLeftOf="@+id/tvHospital1" />

            <TextView
                android:id="@+id/tvHospital1"
                android:textColor="@color/Black"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                app:layout_constraintTop_toTopOf="@+id/ivHospitalPin1"
                app:layout_constraintLeft_toRightOf="@+id/ivHospitalPin1"
                android:layout_marginLeft="16dp"
                android:layout_marginStart="16dp" />

            <ImageView
                android:id="@+id/ivHospitalPin1"
                android:layout_width="32dp"
                android:layout_height="32dp"
                android:layout_margin="15dp"
                android:layout_marginBottom="8dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/ic_location_pin_red"
                android:layout_marginStart="16dp" />

            <ImageView
                android:id="@+id/ivCallHospital1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_phone_black_24dp"
                android:layout_marginRight="16dp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="8dp"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="16dp" />
        </android.support.constraint.ConstraintLayout>
    </android.support.v7.widget.CardView>

</android.support.constraint.ConstraintLayout>